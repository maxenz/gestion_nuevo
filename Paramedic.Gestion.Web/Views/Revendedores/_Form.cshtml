@model Gestion.Models.Revendedor

<div class="main">
    <div class="main-inner">
        <div class="container">
            <div class="span6 offset3">
                <div class="widget">
                    <div class="widget-header">
                        <i class="icon-key"></i>
                        <h3>Registro de Productos</h3>
                    </div>
                    <div class="widget-content">
                        <fieldset>
                            <div class="control-group">
                                <label class="control-label" for="Nombre">Nombre</label>
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Nombre, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Nombre)
                                </div>
                                <!-- /controls -->
                            </div>
                            <!-- /control-group -->

                            <div class="control-group">
                                <label class="control-label" for="Comision">Comisi&oacute;n</label>
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Comision, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Comision)
                                </div>
                                <!-- /controls -->
                            </div>
                            <!-- /control-group -->
                            <div class="control-group">
                                <label class="control-label" for="BajoContrato">Bajo contrato?</label>
                                <div class="controls">
                                    @Html.EditorFor(model => model.BajoContrato, new { @class = "form-control", @placeholder = "Contrato?" })
                                    @Html.ValidationMessageFor(model => model.BajoContrato)
                                </div>
                                <!-- /controls -->
                            </div>
                            <!-- /control-group -->

                            <br>

                            <div class="form-actions">
                                <button type="submit" class="btn btn-primary btn-actions">Guardar</button>
                                <button class="btn">Cancelar</button>
                            </div>
                            <!-- /form-actions -->
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- /container -->
</div>
