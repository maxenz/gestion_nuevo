@model Paramedic.Gestion.Model.ClientesUsuario

<div class="main">
    <div class="main-inner">
        <div class="container">
            <div class="col-xs-6 col-xs-offset-3">
                <div class="widget">
                    <div class="widget-header">
                        
                        <h3>Registro de Usuario</h3>
                    </div>
                    <div class="widget-content">
                        <fieldset>
                            <div class="control-group">
                                <label class="control-label" for="Usuario">Usuario</label>
                                <div class="controls">
                                    @Html.DropDownList("UsuarioID",null, new { @class = "col-xs-5" })
                                    @Html.ValidationMessageFor(model => model.UsuarioId)
                                </div>
                            </div>

                            <div class="control-group">
                                <label class="control-label" for="ShamanExpressID">Shaman Express ID</label>
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.ShamanExpressId, new { @class = "col-xs-12 form-control", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.ShamanExpressId)
                                </div>
                            </div>

                            <div class="control-group">
                                <label class="control-label" for="ShamanFullID">Shaman Full ID</label>
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.ShamanFullId, new { @class = "col-xs-12 form-control", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.ShamanFullId)
                                </div>
                            </div>

                            <br>

                            <div class="form-actions">
                                <button type="submit" class="btn btn-success btn-outline">Guardar</button>
                                <a href="@Url.Action("Index")" class="btn btn-danger btn-outline">Cancelar</a>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
