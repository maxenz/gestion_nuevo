@model PagedList.IPagedList<Gestion.Models.ClientesContacto>
@using PagedList.Mvc; 

<div class="main">
    <div class="main-inner">
        <div class="container">

            <div class="row" style="margin-top: -20px">
                <div class="col-xs-10 col-xs-offset-1">
                    <div class="widget">
                        <div class="widget-header">
                            <i class="fa fa-filter"></i>
                            <h3>Filtros</h3>
                        </div>
                        <!-- /widget-header -->
                        <div class="widget-content" style="margin-top: -10px; padding-bottom: 18px">
                            <form action="@Url.Action("Index")" method="get" id="formSearchLogs" class="searchIndex" data-gestion-ajax="true" data-gestion-target="#contactosList" style="margin-bottom:-10px">
                                <div class="row">
                                    <div class="col-xs-4">
                                        <input type="text" class=" col-xs-3" id="searchName" name="searchName" placeholder="Buscar..." style="width:90%">
                                    </div>
                                    <div class="col-xs-1">
                                        <button class="btn btn-primary btn-small btn-actions" style="width:100%" type="submit" id="search_button" name="search_button"><i class="btn-icon-only fa fa-search"></i></button>
                                    </div>
                                </div>
                            </form>
                            <!-- /bar-chart -->
                        </div>
                        <!-- /widget-content -->
                    </div>
                </div>
            </div>
            @Html.Partial("_Contactos", Model)

        </div>
        <!-- /main-inner -->
    </div>
</div>