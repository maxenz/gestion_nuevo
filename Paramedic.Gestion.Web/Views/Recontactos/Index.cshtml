@model PagedList.IPagedList<Paramedic.Gestion.Model.ClientesGestion>

<div class="main">
    <div class="main-inner">
        <div class="container">
            <div class="row" style="margin-top: -20px">
                <div class="col-xs-10 col-xs-offset-1">
                    <div class="widget">
                        <div class="widget-header">
                            <i class="fa fa-filter"></i>
                            <h3>Filtros</h3>
                        </div>

                        <div class="widget-content" style="margin-top: -10px; padding-bottom: 10px">
                            <form action="@Url.Action("Index")" method="get" class="searchIndex" data-gestion-ajax="true" data-gestion-target="#recontactoList" style="margin-bottom:-10px">
                                <div class="row">
                                    <div class="col-xs-2">
                                        <select id="selTipoGestion" name="selTipoGestion" style="width: 100%">
                                            <option value="1">Todas</option>
                                            <option value="2">Gestiones</option>
                                            <option value="3" selected="selected">Programaci&oacute;n</option>
                                        </select>
                                    </div>
                                    <div class="col-xs-1">
                                        <input type="text" class="form-control datepicker col-xs-1" id="fechaDesde" name="fechaDesde" value="@ViewBag.dftDesde" data-date-format="dd/mm/yyyy" style="width:100%">
                                    </div>
                                    <div class="col-xs-1">
                                        <input type="text" class="form-control datepicker col-xs-1" id="fechaHasta" name="fechaHasta" value="@ViewBag.dftHasta" data-date-format="dd/mm/yyyy" style="width:100%">
                                    </div>

                                    <div class="col-xs-3">
                                        <input type="text" class=" col-xs-3" id="searchName" name="searchName" placeholder="Buscar..." style="width:90%">
                                    </div>
                                    <div class="col-xs-1">
                                        <button class="btn btn-primary btn-small btn-actions" style="width:100%" type="submit" id="search_button" name="search_button"><i class="btn-icon-only fa fa-search"></i></button>
                                    </div>
                                </div>
                            </form>

                        </div>

                    </div>
                </div>
            </div>
            @Html.Partial("_Recontactos", Model)

        </div>

    </div>
</div>



