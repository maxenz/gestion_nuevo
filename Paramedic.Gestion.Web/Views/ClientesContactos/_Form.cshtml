@model Paramedic.Gestion.Model.ClientesContacto

<div class="main">
    <div class="main-inner">
        <div class="container">
            <div class="span6 offset3">
                <div class="widget">
                    <div class="widget-header">
                        <i class="icon-key"></i>
                        <h3>Registro de Contacto</h3>
                    </div>
                    <div class="widget-content">
                        <fieldset>

                            <div class="control-group">
                                @Html.LabelFor(model => model.Nombre, new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Nombre, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Nombre)
                                </div>
                            </div>

                            <div class="control-group">
                                @Html.LabelFor(model => model.Telefono, new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Telefono, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Telefono)
                                </div>
                            </div>

                            <div class="control-group">
                                @Html.LabelFor(model => model.Email, new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Email, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Email)
                                </div>
                            </div>

                            <div class="control-group">
                                @Html.LabelFor(model => model.Otros, new { @class = "control-label" })
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.Otros, new { @class = "span5", autofocus = "" })
                                    @Html.ValidationMessageFor(model => model.Otros)
                                </div>
                            </div>

                            <div class="control-group">
                                @Html.LabelFor(model => model.esInstitucional, new { @class = "control-label" })
                                <div class="controls">
                                    @Html.EditorFor(model => model.esInstitucional)
                                    @Html.ValidationMessageFor(model => model.esInstitucional)
                                </div>
                            </div>

                            <br>

                            <div class="form-actions">
                                <button type="submit" class="btn btn-primary btn-actions">Guardar</button>
                                <button class="btn">Cancelar</button>
                            </div>
                        </fieldset>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
